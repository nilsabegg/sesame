{
  "name": "nilsabegg/sesame",
  "type": "settings",
  "license": "MIT",
  "description": "A PHP library to automate Dropshipping with aliexpress.com as far as possible.",
  "keywords": [
    "PHP",
    "AliExpress",
    "Dropshipping"
  ],
  "authors": [
    {
      "name": "Nils Abegg",
      "email": "nils.abegg@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "symfony/console": "^3.4",
    "symfony/css-selector": "^4.0",
    "symfony/dom-crawler": "^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.6||~6.3",
    "squizlabs/php_codesniffer": "3.*",
    "php-coveralls/php-coveralls": "^2.0",
    "sami/sami": "^4.0",
    "defuse/php-encryption": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "Sesame\\": "./src/Sesame"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Sesame\\Test\\": "./test/Sesame",
      "Sesame\\Test\\Helper\\": "./test/Helper"
    }
  },
  "bin": [
    "./bin/phpsesame"
  ]
}
